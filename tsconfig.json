{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "lib": [
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "moduleResolution": "bundler",
    "types": [
      "jest",
      "react-native"
    ],
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src",
    "App.tsx"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js"
  ],
  "extends": "./node_modules/expo/tsconfig.base.json"
}
